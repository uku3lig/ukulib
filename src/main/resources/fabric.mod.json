{
  "schemaVersion": 1,
  "id": "ukulib",
  "version": "${version}",
  "name": "ukulib",
  "description": "small utility library used in my mods",
  "authors": [
    "uku"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/ukulib",
    "issues": "https://github.com/uku3lig/ukulib/issues",
    "sources": "https://github.com/uku3lig/ukulib"
  },
  "license": "MPL-2.0",
  "icon": "assets/ukulib/icon.png",
  "environment": "client",
  "entrypoints": {
    "modmenu": [
      "net.uku3lig.ukulib.utils.ModMenuRegistrar"
    ]
  },
  "depends": {
    "minecraft": ">=1.21.2",
    "fabricloader": ">=0.16",
    "fabric-resource-loader-v0": "*",
    "fabric-command-api-v2": "*",
    "fabric-key-binding-api-v1": "*"
  },
  "mixins": [
    "ukulib.mixins.json"
  ],
  "accessWidener": "ukulib.accesswidener",
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "links": {
        "modmenu.discord": "https://uku3lig.net/discord"
      }
    }
  }
